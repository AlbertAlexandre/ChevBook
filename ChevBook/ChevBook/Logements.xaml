<Window x:Class="ChevBook.Logements"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChevBook"
        mc:Ignorable="d"
        Title="Logements" Height="300" Width="300" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="99*"/>
            <ColumnDefinition Width="47*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="Log_Modif" Content="Modifier" Margin="57,212,66,0" VerticalAlignment="Top" Click="Log_Modif_Click"/>
        <ListBox x:Name="listBoxLog" HorizontalAlignment="Left" Height="145" Margin="27,41,0,0" VerticalAlignment="Top" Width="235" Grid.ColumnSpan="2" SelectionChanged="listBoxLog_SelectionChanged"/>
        <Button x:Name="LogQuitter" Content="Quitter" Margin="166,212,51,0" VerticalAlignment="Top" Grid.ColumnSpan="2" Click="LogQuitter_Click"/>

    </Grid>
</Window>
